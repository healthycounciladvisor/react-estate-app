.btn-base:hover, .btn-base:focus, .btn-base:active, .btn-base.active, .open .dropdown-toggle.btn-base {
    color: #fff;
    background-color: <%= @current_website.style_variables["action_color"] %>;
    border-color: <%= @current_website.style_variables["action_color"] %>;
}

.fondo_footer {
  background: <%= @current_website.style_variables["footer_bg_color"] || "#333" %>;
  color: <%= @current_website.style_variables["footer_main_text_color"] || "#fff" %>;  
}
.fondo_footer>.pusher {
  /*allows setting of bg color for footer overflow */
  background: <%= @current_website.style_variables["footer_bg_color"] || "#333" %>;
}

ul.list-listings>li.featured {
  border: 1px solid <%= @current_website.style_variables["primary_color"] %>;
}

.fondo_principal {
  background: <%= @current_website.style_variables["primary_color"] %>;
  color: <%= @current_website.style_variables["labels_text_color"] || "#fff" %>;  
}

.fondo_secundario {
  background: <%= @current_website.style_variables["secondary_color"] %>;
  color: <%= @current_website.style_variables["labels_text_color"] || "#fff" %>;  
}

.color_principal,
.color_principal:hover,
.social a:hover,
ul.mega-nav-ul li a i,
nav.venta_alquiler a.selected,
nav.venta_alquiler a:hover {
  color: <%= @current_website.style_variables["primary_color"] %> !important;
}

.contact-info h5 i,
.color_secundario,
.color_secundario:hover {
  color: <%= @current_website.style_variables["secondary_color"] %> !important;
}

.navbar-wp .navbar-nav>li>a:hover,
.navbar-wp .navbar-nav>li>a:focus,
.navbar-wp .navbar-nav>li.selected > a,
.form-base .cuselFrameRight,
.panel-base .panel-heading,
.primary-image:hover,
.thumbnail-images a:hover {
  background: <%= @current_website.style_variables["secondary_color"] %> !important;
  color: #fff;
}

.navbar-wp .navbar-nav > li > a:hover,
.navbar-wp .navbar-nav > li > a:focus {
  color: #fff;
  background-color: <%= @current_website.style_variables["primary_color"] %>;
  border-top: 4px solid <%= @current_website.style_variables["primary_color"] %>;
}

header .text-navbar-brand {
  color: <%= @current_website.style_variables["secondary_color"] %>
}
.ver_inmueble, .action_link {
  color: <%= @current_website.style_variables["action_color"] %>
}

.precio {
  color: <%= @current_website.style_variables["action_color"] %>;
}

.tabs li.active a {
  border-bottom: 3px solid <%= @current_website.style_variables["primary_color"] %>;
}


