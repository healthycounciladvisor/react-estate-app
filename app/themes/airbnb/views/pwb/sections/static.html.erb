<% page_title @current_agency.company_name  %>
<%= render '/pwb/meta_tags' %>
<% content_for :breadcrumb_row do %>
  <nav class="<%= element_classes "breadcrumb_color" %>">
    <div class="nav-wrapper row">
      <div class="col s6">
        <h4>
          <%= I18n.t(@title_key) %>
        </h4>
      </div>
      <div class="col s6">
        <div class="breadcrumb-container right-align">
          <a href="/" >
            <%= I18n.t("webContentSections.home") %>
          </a>
          <a href="#" class="breadcrumb"><%= I18n.t(@title_key) %></a>
        </div>
      </div>
    </div>
  </nav>
<% end %>
<section class="slice bg-white bb static-content-panel">
  <div class="wp-section">
    <div class="container">
      <div class="row">
        <div class="col-md-11">
          <div class="section-title-wr mt-40">
            <h3 class="section-title left">
              <span>
                <%# I18n.t(@title_key) %>
              </span>
            </h3>
          </div>
          <div>
            <p>
              <%== @content.raw %>
            </p>
          </div>
          <div>
            <% if @is_subdomain_admin  %>
              <%= link_to I18n.t("labels.edit"), "#{ "/" + I18n.locale.to_s + @content.urls['adminUrl'] }"  %>
            <% end %>
          </div>
        </div>
        <div class="col-md-1">
        </div>
      </div>
    </div>
  </div>
</section>
